{
  "scripts": {
    "dev": "remix dev",
    "tsc": "tsc",
    "tsc:dev": "tsc --noEmit -w",
    "tailwind": "tailwindcss -o ./app/index.css",
    "tailwind:dev": "tailwindcss -o ./app/index.css --watch",
    "build": "bash scripts/build.sh",
    "deploy": "netlify deploy --prod",
    "prettier": "prettier -w .",
    "prettier:check": "prettier -c .",
    "isort": "tslint -p . --fix",
    "isort:check": "tslint -p .",
    "lint": "pnpm run prettier && pnpm run isort",
    "lint:check": "pnpm run prettier:check && pnpm run isort:check",
    "db:reset": "docker-compose exec mysql mysql -uroot -ppassword -e 'DROP DATABASE IF EXISTS remix_development; CREATE DATABASE remix_development;'"
  },
  "dependencies": {
    "@netlify/functions": "^0.10.0",
    "@remix-run/netlify": "^1.2.2",
    "@remix-run/react": "^1.2.2",
    "@remix-run/server-runtime": "^1.2.2",
    "@sinclair/typebox": "^0.23.4",
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "bcryptjs": "^2.4.3",
    "knex": "^1.0.3",
    "mysql2": "^2.3.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "superjson": "^1.8.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.2",
    "@remix-run/serve": "^1.2.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.2",
    "daisyui": "^2.2.2",
    "esbuild": "^0.14.23",
    "kysely": "^0.17.1",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.5.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  },
  "volta": {
    "node": "16.14.0"
  }
}
